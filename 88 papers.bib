@article{sordo_temperature_2019,
	title = {Temperature amplifies the effect of high {CO}2 on the photosynthesis, respiration, and calcification of the coralline algae Phymatolithon lusitanicum},
	volume = {9},
	issn = {2045-7758 (Print) 2045-7758 (Linking)},
	doi = {10.1002/ece3.5560},
	abstract = {The combination of ocean acidification ({OA}) and global warming is expected to have a significant effect on the diversity and functioning of marine ecosystems, particularly on calcifying algae such as rhodoliths (maerl) that form extensive beds worldwide, from polar to tropical regions. In addition, the increasing frequency of extreme events, such as heat waves, threatens coastal ecosystems and may affect their capacity to fix blue carbon. The few studies where the simultaneous effects of both temperature and {CO}2 were investigated have revealed contradictory results. To assess the effect that high temperature spells can have on the maerl beds under {OA}, we tested the short-time effects of temperature and {CO}2 on the net photosynthesis, respiration, and calcification of the recently described species Phymatolithon lusitanicum, the most common maerl species of southern Portugal. Photosynthesis, calcification, and respiration increased with temperature, and the differences among treatments were enhanced under high {CO}2. We found that in the short term, the metabolic rates of Phymatolithon lusitanicum will increase with {CO}2 and temperature as will the coupling between calcification and photosynthesis. However, under high {CO}2, this coupling will favor photosynthesis over calcification, which, in the long term, can have a negative effect on the blue carbon fixing capacity of the maerl beds from southern Portugal.},
	pages = {11000--11009},
	number = {19},
	journaltitle = {Ecol Evol},
	author = {Sordo, L. and Santos, R. and Barrote, I. and Silva, J.},
	date = {2019-10},
	pmcid = {PMC6802031},
	keywords = {calcification, ocean acidification, photosynthesis, temperature, maerl, respiration, Coralline algae, Co2, heat wave},
	annotation = {Sordo, {LauraSantos}, {RuiBarrote}, {IsabelSilva}, {JoaoengEnglandEcol} Evol. 2019 Sep 13;9(19):11000-11009. doi: 10.1002/ece3.5560. {eCollection} 2019 Oct.},
	annotation = {The following values have no corresponding Zotero field:auth-address: Marine Plant Ecology Research Group, Centre of Marine Sciences ({CCMAR}) University of Algarve Faro Portugal. Portuguese Institute of Ocean and Atmosphere ({IPMA}) Olhao Portugal.edition: 2019/10/24accession-num: 31641450},
}

@article{stearn_calcium_1977,
	title = {Calcium Carbonate Budget of a Fringing Reef on the West Coast of Barbados, Zonation and Productivity},
	volume = {27},
	abstract = {A near-shore dominantly coral-covered area on the west coast of Barbados was surveyed from boats and by diving and a marked correlation between the bathymetry and ecological zonation was apparent. An isolated fringing reef within this surveyed area was selected for budget analyses. The proportion of the substrate covered by dead coral, crustose coralline algae, sand, rubble, and living corals (\textit{Porites porites}, \textit{Porites astreoides}, \textit{Montastrea annularis}, \textit{M. cavernosa}, \textit{Millepora} spp., \textit{Madracis mirabilis}, \textit{Siderastrea siderea}, \textit{Agaricia agaricites}, \textit{Favia fragum}) was determined by measurements along six roped transects. The rocky substrate area was calculated from vertical aerial photographs to give the planimetric area, then this figure was modified by relief factors to take into account the irregular reef surface. The macro-relief factor was determined with ropes and detailed underwater mapping, the micro-relief factor determined using weighted string and modelling clay. The areal coverage in m$^{\textrm{2}}$ for each coral present in greater than 1\% proportional abundance and for the coralline algae was then calculated.{\textless}br{\textgreater}{\textless}/br{\textgreater}The rate of growth of corals was determined either by using X-radiographs of thin slabs cut through axial regions of corals to reveal seasonal banding or by staining with Alizarin red-S. The rate of growth of crustose coralline algae was obtained from measurements made on planted artificial substrates. Once the bulk density of the corals was calculated from simple water-displacement experiments, then the {CaCO}$_{\textrm{3}}$ productivity in gm/m$^{\textrm{2}}$/year could be calculated for each coral. The overall {CaCO}$_{\textrm{3}}$ productivity of the reef was found to be 15 kg {CaCO}$_{\textrm{3}}$/m$^{\textrm{2}}$ planimetric area per year.},
	pages = {479--510},
	number = {3},
	journaltitle = {Bulletin of Marine Science},
	author = {Stearn, C. W. and Scoffin, T. P. and Martindale, W.},
	date = {1977},
}

