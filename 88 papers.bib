	keywords = {Hydrogen-Ion Concentration, Seawater/*chemistry, *biomineralization, *Calcification, Physiologic, *coralline algae, *corals, *environmental variability, *ocean acidification, *resistance to climate change, Animals, Anthozoa/*physiology, Carbonates/*chemistry, Coral Reefs, Rhodophyta/*physiology, Western Australia},
	annotation = {Cornwall, C {EComeau}, {SDeCarlo}, T {MMoore}, {BD}'Alexis, {QMcCulloch}, M {TengResearch} Support, Non-U.S. Gov'{tEnglandProc} Biol Sci. 2018 Aug 8;285(1884). pii: rspb.2018.1168. doi: 10.1098/rspb.2018.1168.},
