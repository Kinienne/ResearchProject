@article{wassenaar_non-invasive_2024,
	title = {Non-invasive determination of critical dissolved oxygen thresholds for stress physiology in fish using triple-oxygen stable isotopes and aquatic respirometry},
	doi = {10.1080/10256016.2024.2366470},
	abstract = {Understanding the critical thresholds of dissolved oxygen (O{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}) that trigger adaptive physiological responses in aquatic organisms is long hampered by a lack of robust, non-lethal or non-invasive methodologies. The isotope fractionation of triple O{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} isotopes ($^{\textrm{18}}$O/$^{\textrm{17}}$O/$^{\textrm{16}}$O) during respiration is linked to the amount of oxygen utilised, offering a potential avenue for new insights. Our experimental research involved measuring the oxygen isotope fractionation of dissolved O{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} in closed-system aquatic respirometry experiments with wild sticklebacks (Gasterosteus aculeatus). These fish were either naturally adapted or experimentally acclimated to hypoxic and normoxic conditions. The aim was to observe their oxygen usage and isotope fractionation in response to increasingly severe hypoxia. Initial observations revealed a progressive $^{\textrm{18}}$O enrichment from the preferential uptake of $^{\textrm{16}}$O to a dissolved oxygen threshold of 3–5 mg O{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} L$^{\textrm{–1}}$, followed by an apparent reversal in oxygen isotope fractionation, which is mixing of $^{\textrm{16}}$O and $^{\textrm{17}}$O with the remaining O{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} pool across all populations and indicative of a systematic change in oxygen metabolism among the fish. Unexpectedly, sticklebacks adapted to hypoxia but acclimated to normoxia exhibited stronger oxygen isotope fractionation compared to those adapted to normoxia and acclimated to hypoxia, contradicting the hypothesis that hypoxia adaptation would lead to reduced isotope discrimination due to more efficient oxygen uptake. These preliminary experimental results highlight the novel potential of using dissolved O{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} isotopes as a non-invasive, non-lethal method to quantitatively assess metabolic thresholds in aquatic organisms. This approach could significantly improve our understanding of the critical oxygen responses and adaptation mechanisms in fish and other aquatic organisms across different oxygen environments, marking a significant step forward in aquatic ecological and physiological research.},
	journaltitle = {Isotopes in Environmental and Health Studies},
	author = {Wassenaar, Leonard I. and Crespel, Amélie and Barth, Johannes and Koeck, Barbara and Závorka, Libor},
	date = {2024},
	keywords = {Respiration, acidosis, adaptation, aquatic animals, hypoxia, physiological stress, plasticity, triple-oxygen isotopes},
	annotation = {{CRIS}-Team Scopus Importer:2024-07-19},
}

@article{dordoni_novel_2022,
	title = {Novel evaluations of sources and sinks of dissolved oxygen via stable isotopes in lentic water bodies},
	volume = {838},
	doi = {10.1016/j.scitotenv.2022.156541},
	abstract = {Dissolved oxygen ({DO}) dynamics of a temperate drinking water reservoir in the Harz Mountains (Germany) were investigated over a time period of 18 months. Via depth profiles in a fortnightly sampling resolution we were able to trace {DO} and temperature dynamics including the formation and breakdown of a Metalimnetic Oxygen Minimum ({MOM}) by means of {DO} concentration, saturation patterns and stable isotope ratios of dissolved oxygen (expressed as δ$^{\textrm{18}}$O{\textless}inf{\textgreater}{DO}{\textless}/inf{\textgreater}). Over the evaluation period, 19.4 \% of the samples collected had δ$^{\textrm{18}}$O{\textless}inf{\textgreater}{DO}{\textless}/inf{\textgreater} values compatible with atmospheric equilibration (+24.6 ‰ ± 0.4 ‰). With values smaller and larger than this threshold, the remaining δ$^{\textrm{18}}$O{\textless}inf{\textgreater}{DO}{\textless}/inf{\textgreater} values showed that 40.8 \% of our samples were dominated by photosynthesis and 39.8 \% by respiration. From December to April the reservoir was mixed and {DO} consumption by respiration exceeded production via photosynthesis. During stratification period, quantification of respiration/photosynthesis rates (R/P) confirmed the epilimnion as a photosynthetic (i.e. net-autotrophic) environment while the hypolimnion was heterotrophic and dominated by respiration at various degrees. Samples of the {MOM} zone showed the highest R/P ratios and had among the most positive δ$^{\textrm{18}}$O{\textless}inf{\textgreater}{DO}{\textless}/inf{\textgreater} signals caused by respiration. This study showed that combinations of {DO} concentrations and their isotope ratios are promising to quantify critical zones of respiration and photosynthesis in aquatic environments.},
	journaltitle = {Science of the Total Environment},
	author = {Dordoni, Marlene and Seewald, Michael and Rinke, Karsten and Schmidmeier, Jakob and Barth, Johannes},
	date = {2022},
	keywords = {Respiration, Atmospheric influence, Lentic water bodies, Metalimnetic Oxygen Minimum ({MOM}), Photosynthesis, R/P ratios},
	annotation = {{CRIS}-Team Scopus Importer:2022-07-01},
}

