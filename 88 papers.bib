	author = {Fine, M. and Tsadok, R. and Meron, D. and Cohen, S. and Milazzo, M.},
	date = {2017-06},
	keywords = {coralline algae, ocean acidification, climate change, macroalgae, carbon-dioxide, assemblages, biodiversity, gastropods, israel, mediterranean sea, neogoniolithon, productivity, vermetid reefs},
