	date = {2009-09-20},
	keywords = {calcification, coralline algae, ocean acidification, photosynthesis, co2, carbon, reef, organisms, alkalinity, calcium, halimeda, hydrolithon sp., ph, impact},
	annotation = {498fkTimes Cited:81Cited References Count:33},
